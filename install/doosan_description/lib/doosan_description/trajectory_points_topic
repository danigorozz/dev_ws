#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'doosan-description','console_scripts','trajectory_points_topic'
__requires__ = 'doosan-description'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('doosan-description', 'console_scripts', 'trajectory_points_topic')()
    )
